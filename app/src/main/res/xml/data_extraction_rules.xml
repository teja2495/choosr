<?xml version="1.0" encoding="utf-8"?><!--
   Data extraction rules file for Android backup and device transfer.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Include all SharedPreferences for cloud backup -->
        <include domain="sharedpref" path="."/>
        
        <!-- Include databases for cloud backup -->
        <include domain="database" path="."/>
        
        <!-- Include files for cloud backup -->
        <include domain="file" path="."/>
    </cloud-backup>
    <device-transfer>
        <!-- Include all SharedPreferences for device transfer -->
        <include domain="sharedpref" path="."/>
        
        <!-- Include databases for device transfer -->
        <include domain="database" path="."/>
        
        <!-- Include files for device transfer -->
        <include domain="file" path="."/>
    </device-transfer>
</data-extraction-rules>